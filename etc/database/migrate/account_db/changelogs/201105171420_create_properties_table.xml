<?xml version="1.0" encoding="utf-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">
  
  <changeSet author="kcando" id="201105171420">
    <createTable tableName="Properties" remarks="key/value information">
      <column name="key" type="VARCHAR(100)">
        <constraints nullable="false"/>
      </column>
      <column name="value" type="VARCHAR(100)"/>
      <column name="comments" type="NVARCHAR(200)"/>
    </createTable>

    <addPrimaryKey tableName="Properties"
      columnNames="key" constraintName="pk_Properties"/>
  </changeSet>
  
</databaseChangeLog>
