<?xml version="1.0" encoding="utf-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">
  
  <changeSet author="kcando" id="201210051143">
    <createTable tableName="Character_Titles" remarks="Character Title table">
      <column name="character_id" type="dbo.id_t">
        <constraints nullable="false"/>
      </column>
      <column name="title_code" type="dbo.code_t">
        <constraints nullable="false"/>
      </column>
    </createTable>
    <addPrimaryKey constraintName="pk_Character_Titles" columnNames="character_id, title_code" tableName="Character_Titles"/>
    

    <addForeignKeyConstraint constraintName="fk_Character_Titles_Characters"
        baseColumnNames="character_id" baseTableName="Character_Titles"
        referencedColumnNames="id" referencedTableName="Characters"
        onDelete="NO ACTION" />
		

  </changeSet>

</databaseChangeLog>
